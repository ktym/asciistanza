    },
    {
      "stanza:key": "--togostanza-font-color",
      "stanza:type": "color",
      "stanza:default": "#333333",
      "stanza:description": "Font color"
    },
    {
      "stanza:key": "--togostanza-background-color",
      "stanza:type": "color",
      "stanza:default": "rgba(255,255,255,0)",
